<Window x:Class="ValorantGUI.AddAgentType"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ValorantGUI"
        mc:Ignorable="d"
        Title="Add Agent Class" Height="163" Width="267" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="SaveButton" Content="Save" HorizontalAlignment="Left" Height="62" Margin="10,53,0,0" VerticalAlignment="Top" Width="233" Click="SaveButton_Click"/>
        <Label x:Name="NameLabel" Content="Agent type name:" HorizontalAlignment="Left" Margin="10,22,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Right" Width="108"/>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Margin="123,22,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="26"/>

    </Grid>
</Window>
